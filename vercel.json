{
    "version": 2,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.php"
      }
    ],
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.4.0"
      }
    }
  }
